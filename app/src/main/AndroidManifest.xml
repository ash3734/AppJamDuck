<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.ash.sopt19th_mate">

    <uses-permission android:name="android.permission.INTERNET" />

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <meta-data
            android:name="com.facebook.sdk.ApplicationId"
            android:value="@string/facebook_app_id" />

        <activity android:name=".login.SplashActivity" />
        <activity
            android:name=".login.MainActivity"
            android:label="@string/title_activity_home"
            android:theme="@style/AppTheme.NoActionBar">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name="com.facebook.FacebookActivity"
            android:configChanges="keyboard|keyboardHidden|screenLayout|screenSize|orientation"
            android:label="@string/app_name"
            android:theme="@android:style/Theme.Translucent.NoTitleBar" />
        <activity android:name=".detail.DetailActivity" />
        <activity android:name=".mypage.MyPageMainActivity" />
        <activity android:name=".HomeActivity" />
        <activity android:name=".setsinger.SetSingerActivity" />
        <activity android:name=".setsinger.ViewHolderActivity" />
        <activity android:name=".dialog.CustomDialogActivity" />
        <activity android:name=".setsinger.SearchViewHolderActivity"></activity>
    </application>

</manifest>